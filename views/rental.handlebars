<!-- For the rental page -->
{{!-- <body> --}}

  <h1>Choose your Rental</h1>

  <div class="rentals-list">
  {{#each vehicles as |vehicle|}}
    <div>
    <div class="rental-card">
      <p>{{vehicle.vehicleType}}</p>
      <p>{{vehicle.id}}</p>
      {{#if vehicle.isAvailable}}
      <p>AVAILABLE</p>
      <a href="/rental/{{vehicle.id}}"><button>Rent Now!</button></a>
      {{else}}
      <p class="available">UNAVAILABLE</p>
      {{/if}}
    </div>
    </div>
  {{/each}}
  </div>

